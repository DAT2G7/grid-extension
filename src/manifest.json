{
	"manifest_version": 3,
	"name": "grid-extension",
	"version": "0.0.1.168259717",
	"description": "Opens a tab contributing to an Aalborg University (DK) grid-computing project",
	"icons": {
		"48": "icons/border-48.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://*/*"
			],
			"js": [
				"content.js"
			]
		}
	],
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"permissions": [
		"storage"
	]
}